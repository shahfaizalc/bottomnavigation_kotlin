<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <stroke android:width="1dp"
        android:color="#85C1E9"
        android:dashWidth="10px"
        android:dashGap="10px"/>

    <solid
        android:width="1dp"
        android:color="#2E86C1"
        />

    <padding android:left="1dp"
        android:top="1dp"
        android:right="1dp"
        android:bottom="1dp"
        />

    <corners android:bottomRightRadius="@dimen/dimen_16" android:bottomLeftRadius="@dimen/dimen_16"
        android:topLeftRadius="@dimen/dimen_16" android:topRightRadius="@dimen/dimen_16"/>
</shape>


